{
	"manifest_version": 2,

	"name": "One-Liners",
	"description": "Get a daily wallpaper and funny one liner on new tab.",
	"version": "0.5",
	"browser_action": {
		"default_icon": "img/icon128.png"
	},
	"icons": { "16": "img/icon16.png",
			   "48": "img/icon48.png",
			   "128": "img/icon128.png" },
	"permissions" : ["management"],
	"chrome_url_overrides" : {
		"newtab": "newtab.html"
	}
 }
